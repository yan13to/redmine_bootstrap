<%= page_title custom_field_title(@custom_field) %>

<%= page_card do %>
  <%= render 'form' %>
<% end %>

<%= javascript_tag do %>
$('#custom_field_field_format').change(function(){
  $.ajax({
    url: '<%= new_custom_field_path(:format => 'js') %>',
    type: 'get',
    data: $('#custom_field_form').serialize(),
    complete: toggleDisabledInit
  });
});
<% end %>
