<%= page_table  do %>
  <%= page_table_thead do %>
    <%= page_table_tr do %>
      <% [l(:field_name), l(:field_type), l(:field_host), l(:label_user_plural), nil].each do |th_label| %>
        <%= page_table_th do %>
          <%= th_label %>
        <% end %>
      <% end %>
    <% end %>
  <% end %>
  <%= page_table_tbody do %>
    <%= render partial: 'source', collection: @auth_sources %>
  <% end %>
<% end %>
